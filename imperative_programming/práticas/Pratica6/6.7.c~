#include<stdio.h>

int main() {

  int vet[50]={0} ,valor=0, i, conta++, j, flag;

  for (i=0 ; i<50 && valor!= -1 ;i++) { // le valor atÃ© ficar cheio, ou terminar com -1
    scanf("%d" ,&valor);
    for (j=0 ; vet[j]!=0 ; i++) {    //verifica se o valor se encontra na seq
      if (valor == vet[j])    
	flag=1;
    }
    if (flag!=1)
      ve[i] = valor;
    flag=0;
  }
  for (i=0; vet[i] != 0; i++)
    conta++;
  printf("%d" ,&conta);
  return 0;
}
